
@{
    ViewData["Title"] = "UploadBlob";
}

<h1 class="display-6">Página para Upload de Blobs</h1>

<div class="row">
	<div class="col-md-6">
		<form asp-action="UploadBlob" enctype="multipart/form-data">
			<div class="mb-3">
				<label class="control-label">Escolha uma imagem</label>
				<input type="file" class="form-control" name="blob" />
			</div>
			<button type="submit" class="btn btn-secondary">Upload</button>
		</form>
	</div>
</div>

